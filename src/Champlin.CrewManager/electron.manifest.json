{
  "executable": "Champlin.CrewManager",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "Champlin.CrewManager",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.Champlin.CrewManager.app",
    "productName": "Champlin.CrewManager",
    "copyright": "Copyright Â© 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "win": {
      "target": "portable"
    },
    "portable": {
      "unicode": false,
      "artifactName": "Champlin-CrewManager.exe"
    },
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}